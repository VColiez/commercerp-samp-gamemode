stock AranmaKaydet()
{
	new query[100];
	for(new i = 0; i < MAX_ARANMA; i++) if(Aranmalar[i][aExists])
	{
		format(query, 100, "UPDATE `aranmalar` SET `aEkleyen` = '%s', `aEklenen` = '%d', `aIcerik` = '%s', `aTarih` = '%s'", Aranmalar[i][aEkleyen], Aranmalar[i][aEklenen], Aranmalar[i][aIcerik], Aranmalar[i][aTarih]);
		mysql_query(g_SQL, query, false);
	}
	return 1;
}
stock AranmaSil(id)
{
	if(Aranmalar[id][aExists])
	{
		Aranmalar[id][aExists] = false;
		new query[100];
		format(query, 100, "DELETE FROM `aranmalar` WHERE `aID` = '%d'", Aranmalar[id][aID]);
		mysql_query(g_SQL, query, false);
	}
	return 1;
}
stock AranmaOlustur(issuerid, icerik[], playerid)
{
		new query[300];
		format(query, 300, "INSERT INTO `aranmalar` (`aEkleyen`, `aEklenen`, `aIcerik`, `aTarih`) VALUES ('%s', '%d', '%s', '%s')", ReturnName(playerid), PlayerData[issuerid][pID], icerik, ReturnDate());
		mysql_query(g_SQL, query, false);
		for(new i = 0; i < MAX_ARANMA; i++) if(!Aranmalar[i][aExists])
		{
			Aranmalar[i][aID] = i;
			Aranmalar[i][aExists] = true;
			Aranmalar[i][aEklenen] = PlayerData[issuerid][pID];
			format(Aranmalar[i][aEkleyen], 50, ReturnName(playerid));
			format(Aranmalar[i][aTarih], 50, ReturnDate());
			format(Aranmalar[i][aIcerik], 128, icerik);
			return i;
		}
	return -1;
}

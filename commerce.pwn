	                Sirket_Sil(i);
	            }
	            if(sirketData[i][sirketKasa] >= sirketData[i][sirketVergi])
	            {
             		if(IsimdenSQLID(sirketData[i][sirketSahip]) != -1)
					 {
		    			new bildiri[160];
						format(bildiri, 160, "Þirketinize $%d miktar vergi geldi ve bu miktar þirket kasasýndan kesildi.", sirketData[i][sirketVergi]);
			     		BildirimEkle(IsimdenSQLID(sirketData[i][sirketSahip]), bildiri, "Sistem");
			     	}
					sirketData[i][sirketKasa] = sirketData[i][sirketKasa] - sirketData[i][sirketVergi];
					sirketData[i][sirketVergiZaman] = gettime() + 1296000;
				}
	        }
	        if(sirketData[i][sirketAktifGT] >= gettime())
	        {
	            foreach(new j : Player)
	            {
	            	if(PlayerData[j][pSirket] == sirketData[i][sirketID])
	                	PlayerData[j][pSirketAktif] = 0;
	            }
	            sirketData[i][sirketAktifGT] = gettime() + 168*3600;
	            Sirket_Kaydet(i);
	        }
		}
	}
	return 1;
}

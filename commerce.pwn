	if(GetPlayerVehicleSeat(playerid) != 0 && IsPlayerInAnyVehicle(playerid)) ozeldurum = true;
	new Float:dist = GetPlayerDistanceFromPoint(playerid, sonKonum[playerid][0], sonKonum[playerid][1], sonKonum[playerid][2]);

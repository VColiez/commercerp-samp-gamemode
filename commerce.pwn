stock Sirala(dizi[], u = sizeof (dizi), bool:tip = false, limit = 10)
{
	new
	    kukla[MAX_PLAYERS + 1],
	    kukIndex
	;
	for(new i; i < u; i++)
	{
		for(new j; j < u; j++)
		{
			if(dizi[i] < dizi[j])
			{
   				kukIndex = kukIndex + 1;
			}
			if(j == (u - 1))
			{
   			    if(kukIndex == 0)
			    {
       				kukla[0] = dizi[i];
			    }
			    else
			    {
					kukla[kukIndex] = dizi[i];
 					kukIndex = 0;
		    	}
		    }
		}
		if(i == (u - 1))
		{
		    if(!tip)
		    {
				 for(new k; k < u ; k++)
		         {
				    if(k > (limit - 1))
				        continue;
				    else
					return kukla[k];
				 }
		    }
		    else
		    {
      			 for(new k = (u - 1), m = k; k > -1; k--)
		         {
				    if(k <= (m - limit))
				        continue;
				    else
					return kukla[k];
		         }
            }
      	}
  	}
  	return -1;
}

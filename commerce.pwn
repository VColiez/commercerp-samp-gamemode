stock GetVehicleSpeedExHack(Float:x, Float:y, Float:z)
{
    new vel;
    vel = floatround(floatsqroot( x*x + y*y + z*z ) * 180); // KM/H
    return vel;
}

stock GetVehicleSpeedExHackk(Float:x, Float:y, Float:z)
{
    new vel;
    vel = floatround(floatsqroot( x*x + y*y + z*z ) * -20); // KM/H
    return vel;
}

stock CheckDrive(vehicleid)
{
    foreach(Player, i){
        if(IsPlayerInAnyVehicle(i)){
            if(GetPlayerVehicleID(i) == vehicleid){
                if(GetPlayerState(i) == PLAYER_STATE_DRIVER){
                    return 1;

	if(AracatPos[playerid][0] == -1 && AracatPos[playerid][1] == -1 && AracatPos[playerid][2] == -1) return 0;
	if(!OyundaDegil(playerid) || !OyundaDegil(id)) return 0;
	new vehicleid = GetNearestVehicle(playerid);
	if(vehicleid == INVALID_VEHICLE_ID) return HataMesajGonder(playerid, "Herhangi bir araca yakýn deðilsiniz.");
	if(GetVehicleMaxSeats(vehicleid) < 2) return HataMesajGonder(playerid, "Bu araç dolu.");

	}
	else
	{
	    if (GetFactionType(playerid) == BIRLIK_LSPD || GetFactionType(playerid) == BIRLIK_FBI || GetFactionType(playerid) == BIRLIK_SALS || GetFactionType(playerid) == BIRLIK_LSCSD || GetFactionType(playerid) == BIRLIK_LSMD)
	    {
		    PlayerData[userid][pDragged] = 1;
		    PlayerData[userid][pDraggedBy] = playerid;
            suruklenenpic[playerid] = userid;
		    PlayerData[userid][pDragTimer] = SetTimerEx("DragUpdate", 200, true, "dd", playerid, userid);
		    SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s, %s adli kisiyi sürüklemeye baslar.", ReturnName(playerid, 0), ReturnName(userid, 0));
	    }
		else
		{
			PlayerData[playerid][pSuruklemeyeCalisiyo] = 5;
			PlayerData[playerid][pSuruklemeyeCalistigiID] = userid;
			ApplyAnimation(playerid, "COP_AMBIENT", "Copbrowse_loop", 4.1, 1, 0, 0, 0, 0, 1);
			SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s, %s adlý kiþiyi sürüklemeye çalýþýr.", ReturnName(playerid, 0), ReturnName(userid, 0));
		}
	}
	return 1;
}


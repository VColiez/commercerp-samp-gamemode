CMD:cop(playerid, params[])
{
	if(IsPlayerInAnyVehicle(playerid)) return HataMesajGonder(playerid, "Bu komutu araçta kullanamazsýnýz.");
	new vehicleid = GetPVarInt(playerid, "LastVehicleID");
	if(GetVehicleModel(vehicleid) != 408) return HataMesajGonder(playerid, "Son bindiðiniz araç çöp aracý olmalýdýr.");
    if(HasTrash[playerid]) return HataMesajGonder(playerid, "Zaten bir çöp torbasý taþýyorsunuz.");
	new id = Trash_ClosestEx(playerid);
	if(id == -1) return HataMesajGonder(playerid, "Yakýnýnýzda çöp kutusu yok.");
	if(TrashData[id][TrashLevel] < 1) return HataMesajGonder(playerid, "Bu çöp kutusu boþ.");
    new Float: x, Float: y, Float: z;
    GetVehicleBoot(vehicleid, x, y, z);
    if(GetPlayerDistanceFromPoint(playerid, x, y, z) >= 30.0) return HataMesajGonder(playerid, "Yakýnýzda çöp aracý yok.");
	TrashData[id][TrashLevel]--;
	KillTimer(TrashData[id][TrashTimer]);
    TrashData[id][TrashTimer] = SetTimerEx("FillTrash", REFILL_TIME * 1000, false, "i", id);
	TrashCP[playerid] = CreateDynamicCP(x, y, z, 3.0, .playerid = playerid);
	HasTrash[playerid] = true;
	ApplyAnimation(playerid, "CARRY", "liftup105", 4.1, 0, 0, 0, 0, 0);
	SetPlayerAttachedObject(playerid, ATTACHMENT_INDEX, 1264, 6, 0.222, 0.024, 0.128, 1.90, -90.0, 0.0, 0.5,0.5, 0.5);
	Streamer_SetIntData(STREAMER_TYPE_3D_TEXT_LABEL, TrashData[id][TrashLabel], E_STREAMER_COLOR, (TrashData[id][TrashLevel] == 0) ? 0xE74C3CFF : 0xF39C12FF);
	MesajGonder(playerid, "Belirtilen tuþa basarak çöpü araca býrakýn. Çöp torbasýný býrakmak için; ~k~~CONVERSATION_NO~");
	return 1;
}



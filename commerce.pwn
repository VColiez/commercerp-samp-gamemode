Dialog:CalisanOnay(playerid, response, listitem, inputtext[])
{
	new id = INVALID_PLAYER_ID;
	foreach(new i : Player) if(GetPVarInt(i, "gereklipvar") == playerid)
	    id = i;
	if(id == INVALID_PLAYER_ID) return HataMesajGonder(playerid, "Hata oluþtu.");
	new rank = GetPVarInt(id, "gereklipvar2");
	if(rank == -1) return HataMesajGonder(playerid, "Hata oluþtu.");
	if(!response)
	{
	    MesajGonder(id, "%s adlý oyuncu çalýþan davetinizi redetti.", ReturnName(playerid));
	    MesajGonder(playerid, "%s adlý oyuncunun çalýþan davetini redettiniz.", ReturnName(id));
	}
	if(response)
	{
	    if(strval(inputtext) == GetPVarInt(playerid, "OnayKOD"))
	    {
			new bizid = Business_Inside(playerid);
			new ids = -1;
			if(bizid == -1) return HataMesajGonder(playerid, "Ýþyerinde deðilsiniz.");
			for(new i = 0; i < 21; i++)
			{
			    if(iCalisanlar[bizid][i] == -1)
			    {
					ids = i;
			        iCalisanlar[bizid][i] = PlayerData[playerid][pID];
			        iRanklar[bizid][i] = rank;
			        break;
			    }
			}
			if(ids == -1) return HataMesajGonder(playerid, "Olaðan hata durumu #2");
		    PlayerData[playerid][iCalisan] = Isyeri[bizid][isyeriID];
		    PlayerData[playerid][iRank] = rank;
			PlayerData[playerid][iRozet] = 0;
			MesajGonder(playerid, "%s adlý iþyerinde %s mevkisinde çalýþmaya baþladýnýz.", Isyeri[bizid][isyeriName], CalisanTip(rank));
			MesajGonder(id, "%s adlý oyuncu çalýþan davetinizi kabul etti.", ReturnName(playerid));
		}
	}
	DeletePVar(id, "gereklipvar2");
	DeletePVar(id, "gereklipvar");
	DeletePVar(id, "CalisanYonet");
	return 1;
}

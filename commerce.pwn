forward MDDragUpdate(playerid, targetid);
public MDDragUpdate(playerid, targetid)
{
	if (PlayerData[targetid][pDragged] && PlayerData[targetid][pDraggedBy] == playerid)
	{
	    static Float:fX, Float:fY, Float:fZ, Float:fAngle;

		GetPlayerPos(playerid, fX, fY, fZ);
		GetPlayerFacingAngle(playerid, fAngle);

		fX -= 1.5 * floatsin(-fAngle, degrees);
		fY -= 1.5 * floatcos(-fAngle, degrees);

		SetDynamicObjectPos(Sedye[playerid], fX, fY, fZ - 0.5);
		SetDynamicObjectRot(Sedye[playerid], 0.0000000, 0.0000000, fAngle);
		PlayerData[targetid][pLegalTeleport] = gettime() + 2;
		SetPlayerPos(targetid, fX, fY, fZ + 1.1000);



		SetPlayerInterior(targetid, GetPlayerInterior(playerid));
		SetPlayerVirtualWorld(targetid, GetPlayerVirtualWorld(playerid));
	}
	return 1;
}

forward DragUpdate(playerid, targetid);
public DragUpdate(playerid, targetid)
{
	if (PlayerData[targetid][pDragged] && PlayerData[targetid][pDraggedBy] == playerid)
	{
	    static
	        Float:fX,
	        Float:fY,
	        Float:fZ,
			Float:fAngle;
		if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
		{
            PlayerData[targetid][pDragged] = 0;
	    	PlayerData[targetid][pDraggedBy] = INVALID_PLAYER_ID;
	    	PlayerData[playerid][pSuruklemeyeCalisiyo] = 0;
			PlayerData[playerid][pSuruklemeyeCalistigiID] = -1;
        	suruklenenpic[targetid] = -1;
	    	KillTimer(PlayerData[targetid][pDragTimer]);
	    	SendNearbyMessage(playerid, 30.0, COLOR_CYAN, "** %s, %s adlý kiþiyi sürüklemeyi býrakýr.", ReturnName(playerid, 0), ReturnName(targetid, 0));
	    	MesajGonder(playerid, "Araca bindiðiniz için sürükleme iþlemi iptal edildi.");
		}
		new id = House_Inside(playerid);
		if(id != -1)
		{
		    SetPlayerInterior(targetid, HouseInteriors[ HouseInfo[id][evInterior] ][intID]);
		    PlayerData[targetid][pHouse] = id;
			SetPlayerVirtualWorld(targetid,HouseInfo[id][evID]+1);
		}
		GetPlayerPos(playerid, fX, fY, fZ);
		GetPlayerFacingAngle(playerid, fAngle);

		fX -= 1.5 * floatsin(-fAngle, degrees);
		fY -= 1.5 * floatcos(-fAngle, degrees);
		PlayerData[targetid][pLegalTeleport] = gettime() + 2;
		SetPlayerPos(targetid, fX, fY, fZ);
	}
	return 1;
}


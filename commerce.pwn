GetLiderOyuncu(oda)
{
	new PlayerID = -1, oldurme, olme;
	foreach (new i:Player)
	{
	    if (IsPlayerConnected(i) && pbOda[i] == oda)
	    {
	        if (pbOldurme[i] > oldurme || (pbOldurme[i] == oldurme && pbOlme[i] < olme))
	        {
				PlayerID = i;
				oldurme = pbOldurme[i];
				olme = pbOlme[i];
	        }
	    }
	}
	return PlayerID;
}

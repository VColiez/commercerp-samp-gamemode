				Dialog_Show(playerid, RehberAyar, DIALOG_STYLE_LIST, "Rehber", "{38c5f7}» {FFFFFF}Ara\n{38c5f7}» {FFFFFF}SMS Yolla\n{38c5f7}» {FFFFFF}Sil", "Seç", "Geri");
			}
		}
	}
	return 1;
}

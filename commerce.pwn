		foreach (new i : Player) if ((GetFactionType(i) == BIRLIK_LSPD) || GetFactionType(i) == BIRLIK_LSMD || GetFactionType(i) == BIRLIK_FBI || GetFactionType(i) == BIRLIK_SALS || GetFactionType(i) == BIRLIK_LSCSD) {

CMD:yemver(playerid, params[])
{
	new id, YemTuru, YemMiktar;

    if (PlayerData[playerid][pAdmin] < 3)
		return HataMesajGonder(playerid, "Yetkiniz yok!");

	if (sscanf(params, "udd", id, YemTuru, YemMiktar))
	    return BilgiMesajGonder(playerid, "/yemver [Oyuncu Isim/ID] [2 = Karides | 1 = Sülünes | 0 = Ekmek Kýrýntýsý] [Miktar]");

	if (YemTuru < 0 || YemTuru > 2)
	    return HataMesajGonder(playerid, "Yem Türüne maksimum 3, minimum 1 yazabilirsin.");

	if (YemMiktar < 1 || YemMiktar > 30)
	    return HataMesajGonder(playerid, "Yem miktarý minimum 1, maksimum 30 olabilir.");

	PlayerData[id][pYemTurleri][YemTuru] += YemMiktar;

	switch (YemTuru)
	{
		case 0:
		{
			MesajGonder(playerid, "%s isimli oyuncuya %d tane Ekmek Kýrýntýsý yemi verdiniz.", ReturnName(id, 0), YemMiktar);
			MesajGonder(id, "%s isimli yetkiili size %d tane Ekmek Kýrýntýsý yemi verdi.", ReturnName(playerid, 0), YemMiktar);
		}
        case 1:
		{
			MesajGonder(playerid, "%s isimli oyuncuya %d tane Sülünes yemi verdiniz.", ReturnName(id, 0), YemMiktar);
			MesajGonder(id, "%s isimli yetkiili size %d tane Sülünes yemi verdi.", ReturnName(playerid, 0), YemMiktar);
		}

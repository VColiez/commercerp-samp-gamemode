const PLAYER_SYNC = 207;
public OnIncomingPacket(playerid, packetid, BitStream:bs)
{

    if (packetid == PLAYER_SYNC)
    {
        new onFootData[PR_OnFootSync];

        BS_IgnoreBits(bs, 8);
        BS_ReadOnFootSync(bs, onFootData);

		if(onFootData[PR_animationId] == 400)
		{
		    AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), animasyonlu airbreak kullandýðý için sunucudan atýldý.(DAM_LAUNCH)", ReturnName(playerid, 0), playerid);
		    MesajGonder(playerid,"Actor bozukluðu tespit edildiði için oyundan atýldýn.");
		    Kick(playerid);
		}
		if(onFootData[PR_quaternion][0] <= 0.01 && onFootData[PR_position][0] == 0.00 &&onFootData[PR_position][1] == 0.00 && onFootData[PR_position][2] == 0.00)
		{
		    AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), crasher denediði için sunucudan atýldý.", ReturnName(playerid, 0), playerid);
		    MesajGonder(playerid,"Actor bozukluðu tespit edildiði için oyundan atýldýn.");
		    Kick(playerid);
		    return 0;
		}
		if(wankdurum[playerid] == false && onFootData[PR_animationId] == 993)
		{
		    AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), animasyonlu airbreak kullandýðý için sunucudan atýldý.(WANK2)", ReturnName(playerid, 0), playerid);
		    MesajGonder(playerid,"Actor bozukluðu tespit edildiði için oyundan atýldýn.");
		    Kick(playerid);
		}
        if (onFootData[PR_surfingVehicleId] != 0 && onFootData[PR_surfingVehicleId] != INVALID_VEHICLE_ID)
		{
            if ((floatabs(onFootData[PR_surfingOffsets][0]) >= 50.0) || (floatabs(onFootData[PR_surfingOffsets][1]) >= 50.0) || (floatabs(onFootData[PR_surfingOffsets][2]) >= 50.0))
			{
                onFootData[PR_surfingOffsets][0] = 0.0;
                onFootData[PR_surfingOffsets][1] = 0.0;
                onFootData[PR_surfingOffsets][2] = 0.0;

                BS_SetWriteOffset(bs, 8);
                BS_WriteOnFootSync(bs, onFootData);
            }
		}
    }
    if (packetid == 203)
    {
        new aimData[PR_AimSync];

        BS_IgnoreBits(bs, 8);
        BS_ReadAimSync(bs, aimData);

        if (aimData[PR_aimZ] != aimData[PR_aimZ])
        {
            return 0;
        }
        if (aimData[PR_camMode] == 45 || aimData[PR_camMode] == 34)
        {
            return 0;
    	}
    }

	if(packetid == 206)
	{
  		new BulletData[PR_BulletSync];
  		new Float:fcenterX;
  		new Float:fcenterY;
  		new Float:fcenterZ;

  		BS_IgnoreBits(bs, 8);
  		BS_ReadBulletSync(bs, BulletData);
  		fcenterX = BulletData[PR_offsets][0];
  		fcenterY = BulletData[PR_offsets][1];
  		fcenterZ = BulletData[PR_offsets][2];
  		if(fcenterX < 0.00)
  		{
  			fcenterX -= 0.01;
  		}
  		else
  		{
 			fcenterX += 0.01;
  		}
  		if(fcenterY < 0.00)
  		{
  			fcenterY -= 0.01;
  		}
  		else
  		{
 			fcenterY += 0.01;
  		}
  		if(fcenterZ < 0.00)
  		{
  			fcenterZ -= 0.01;
  		}
  		else
  		{
 			fcenterZ += 0.01;
  		}
  		Log_Write("logs/datas.txt", "Bullet Data:[%s] HITTYPE:%d HITID:[%d] ORIGIN: %.2f %.2f %.2f HITPOS: %.2f %.2f %.2f FCENTER: %0.2f %0.2f %0.2f",ReturnName(playerid, 0),
  		BulletData[PR_hitType],BulletData[PR_hitId], BulletData[PR_hitId],BulletData[PR_origin][0], BulletData[PR_origin][1], BulletData[PR_origin][2],
  		BulletData[PR_hitPos][0], BulletData[PR_hitPos][1], BulletData[PR_hitPos][2],fcenterX, fcenterY, fcenterZ);


  		if (BulletData[PR_offsets][0] == 0.01 &&
            BulletData[PR_offsets][1] == 0.02 &&
            BulletData[PR_offsets][2] == -0.04 )
  		{
    		AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), Silent Aim kullanmaya çalýþýyor. (CODE-1) (bullet distance: %0.2f %0.2f %0.2f)", ReturnName(playerid, 0), playerid, fcenterX, fcenterY, fcenterZ);
    		return 0;
  		}
  		if (BulletData[PR_offsets][0] == -0.01 &&
            BulletData[PR_offsets][1] == -0.02 &&
            BulletData[PR_offsets][2] == 0.04 )
  		{
    		AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), Silent Aim kullanmaya çalýþýyor. (CODE-16) (bullet distance: %0.2f %0.2f %0.2f)", ReturnName(playerid, 0), playerid, fcenterX, fcenterY, fcenterZ);
    		return 0;

  		}
  		if(BulletData[PR_offsets][0] == -0.10 &&
            BulletData[PR_offsets][1] == -0.90 &&
            BulletData[PR_offsets][2] == -0.12)
    	{
    		AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), Silent Aim kullanmaya çalýþýyor. (CODE-17) (bullet distance: %0.2f %0.2f %0.2f)", ReturnName(playerid, 0), playerid, fcenterX, fcenterY, fcenterZ);
    	    return 0;

		}

  		if(BulletData[PR_hitType] == 4)
  		{
    		AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), Silent Aim kullanmaya çalýþýyor. (CODE-18)(bullet distance: %0.2f %0.2f %0.2f)", ReturnName(playerid, 0), playerid, fcenterX, fcenterY, fcenterZ);

    		return 0;
  		}

  		if(BulletData[PR_hitType] == 0 && BulletData[PR_hitId] != 65535)
  		{
    		AdminMessage(COLOR_LIGHTRED, "ADM: %s (%d), Silent Aim kullanmaya çalýþýyor. (CODE-19) (bullet distance: %0.2f %0.2f %0.2f)", ReturnName(playerid, 0), playerid, fcenterX, fcenterY, fcenterZ);

    		return 0;
  		}

	}
    return 1;
}




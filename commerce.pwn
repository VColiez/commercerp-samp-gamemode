		{
			AdminMessage(COLOR_LIGHTRED, "ADM: %s(%d) adlý oyuncu dcar hilesi kullanýyor olabilir specle.", ReturnName(i), i);
		}
		AdminMessage(COLOR_LIGHTRED, "ADM: %s(%d) model araç dcar hilesine maruz kaldý.", ReturnVehicleModelName(AracInfo[vehicle][aracModel]), vid);
		new Float:Pos[3];
		GetVehiclePos(vid, Pos[0], Pos[1], Pos[2]);

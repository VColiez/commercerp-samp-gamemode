	if (!weaponid)
	    return HataMesajGonder(playerid, "Silah vermek için silahýn elinde olmasý gereklidir.");

	if (PlayerData[playerid][pBaygin] == 1)
	    return HataMesajGonder(playerid, "Yaralýyken veya ölüyken bu komutu kullanamazsýn.");

	if (sscanf(params, "ud", userid, fiyat))
	    return BilgiMesajGonder(playerid, "/silahsat [hedef id/isim] [fiyat]");


	if (isnull(params)) return BilgiMesajGonder(playerid, "/yayin [Yazý]");
	if (strlen(params) > 64)
	{
	    foreach (new i : Player) if ((PlayerData[i][pDinle] == PlayerData[playerid][pFaction] || i == playerid) && PlayerData[i][pAyarlar][2] == 0)

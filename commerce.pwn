public OnDynamicObjectMoved(objectid)
{
	for (new i = 0; i < MAX_GISE; i++)
	{
	    if (Gise[i][gs_Exists])
	    {
	        if (objectid == Gise[i][gs_Obje])
	        {
	            if (Gise[i][gs_Durum] == GISE_ACIK)
	            {

	new seatid = GetAvailableSeat(vehicleid, 2);
	if (seatid == -1) return HataMesajGonder(playerid, "Boþ koltuk yok.");
	if (PlayerData[playerid][pDragged] == 1)
	{
		PlayerData[playerid][pDragged] = 0;
		PlayerData[playerid][pDraggedBy] = INVALID_PLAYER_ID;
	}
	new yazi[50];
	format(yazi, 50, "kapýyý açar ve %s'i koltuða oturtur.", ReturnName(id));
	cmd_me(playerid, yazi);
	TogglePlayerControllable(id, 0);
	ClearAnimations(id);
	PutPlayerInVehicle(id, vehicleid, seatid);
	return 1;
}

stock OyuncuBaliklariCMD(playerid)
{
	new str[1024], string[228];
	strcat(str, "{FFFFFF}Balýk\t{FFFFFF}Gram\t{FFFFFF}Fiyat\n");
	new baliklar = 0, fiyat;

	for (new i; i < 10; i++)
	{
	    if (PlayerData[playerid][pBalikID][i] != -1)
	    {
	        fiyat = floatround(PlayerData[playerid][pBalikGram][i] * BalikTurleri[PlayerData[playerid][pBalikID][i]][GramFiyat]);

	        format(string, sizeof string, "{FFFFFF}%s\t{FFFFFF}%d\t{00FF00}%s\n", BalikTurleri[PlayerData[playerid][pBalikID][i]][TurAdi], PlayerData[playerid][pBalikGram][i], (GetServerBilgi("2xBalik") != 1) ? (FormatNumber(fiyat)) : (FormatNumber(fiyat*2)));
	        strcat(str, string);

	        baliklar++;
	    }
	}


	if (baliklar < 1) return HataMesajGonder(playerid, "Hiç balýðýnýz yok.");
	format(string, sizeof string, "{FFFFFF}Balýklarýnýz {FF0000}(%d/10)",baliklar);

			{
		  		format(kstr, sizeof(kstr), "%d\t\t%s\t\t%s\t\t%s\t\t%d\n", IndirimliAraclar[i][aracID], ReturnVehicleModelName(IndirimliAraclar[i][Model]), FormatNumber(IndirimliAraclar[i][Fiyat]), sirketData[IndirimliAraclar[i][sirketID]][sirketAd], IndirimliAraclar[i][Kalan]);
